        <application android:networkSecurityConfig="@xml/network_security_config" />
    </edit-config>
    <resource-file src="res/android/xml/network_security_config.xml" target="app/src/main/res/xml/network_security_config.xml" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="DefaultHostname" value="regouga.outsystemscloud.com" />
    <preference name="DefaultApplicationURL" value="PocketFestival" />
    <preference name="AppendUserAgent" value="OutSystemsApp v.0.1" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="ShowSplashScreen" value="false" />
    <preference name="SplashScreenDelay" value="0" />
    <preference name="BackgroundColor" value="#ffdc3023" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#ff000000" />
    <preference name="DefaultPlatformVersion" value="11.0.118.0" />
    <preference name="NativeShellVersion" value="5.0.2" />
    <preference name="android-minSdkVersion" value="21" />
    <preference name="ErrorUrl" value="file:///android_asset/www/error.html" />
    <preference name="loadUrlTimeoutValue" value="60000" />
</widget>