    </feature>
    <feature name="OSSecurity">
        <param name="android-package" value="com.outsystems.plugins.ossecurity.OSSecurity" />
        <param name="onload" value="true" />
    </feature>
    <feature name="OSApplicationInfo">
        <param name="android-package" value="com.outsystems.plugins.applicationinfo.OSApplicationInfoPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="OSLogger">
        <param name="android-package" value="com.outsystems.plugins.oslogger.OSLoggerPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="OSCrashHandler">
        <param name="android-package" value="com.outsystems.plugins.crashhandler.OSCrashHandlerPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="OSCache">
        <param name="android-package" value="com.outsystems.plugins.oscache.OSCache" />
        <param name="onload" value="true" />
    </feature>
    <feature name="OSManifestParser">
        <param name="android-package" value="com.outsystems.plugins.manifest.OSManifestParser" />
    </feature>
    <feature name="OSPreBundle">
        <param name="android-package" value="com.outsystems.plugins.prebundle.OSPreBundlePlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="OSCordovaLoader">
        <param name="android-package" value="com.outsystems.plugins.loader.OSCordovaLoader" />
        <param name="onload" value="true" />
    </feature>
    <feature name="OSDeepLinks">
        <param name="android-package" value="com.outsystems.plugins.deeplinks.OSDeepLinks" />
        <param name="onload" value="true" />
    </feature>
    <name>Pocket Festival</name>
    <description>Native application generated by OutSystems</description>
    <author email="support@outsystems.com" href="http://www.outsystems.com">OutSystems Inc.</author>
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="data:*" />
    <access origin="*" />
    <content src="https://regouga.outsystemscloud.com/PocketFestival/" />
    <hook src="hooks/applyPatches.js" type="after_platform_add" />
    <hook src="hooks/clearCaches.js" type="before_build" />
    <allow-intent href="market:*" />
    <splash density="land-ldpi" src="res/android/drawable-land-ldpi/screen.png" />
    <splash density="land-mdpi" src="res/android/drawable-land-mdpi/screen.png" />
    <splash density="land-hdpi" src="res/android/drawable-land-hdpi/screen.png" />
    <splash density="land-xhdpi" src="res/android/drawable-land-xhdpi/screen.png" />
    <splash density="land-xxhdpi" src="res/android/drawable-land-xxhdpi/screen.png" />
    <splash density="land-xxxhdpi" src="res/android/drawable-land-xxxhdpi/screen.png" />
    <splash density="port-ldpi" src="res/android/drawable-port-ldpi/screen.png" />
    <splash density="port-mdpi" src="res/android/drawable-port-mdpi/screen.png" />
    <splash density="port-hdpi" src="res/android/drawable-port-hdpi/screen.png" />
